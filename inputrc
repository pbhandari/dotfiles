set -o vi

"\e[A": history-search-backward
"\e[B": history-search-forward
"\e[3~": delete-char

# Home and End
# urxvt
"\e[8~": end-of-line
"\e[7~": beginning-of-line
#Gnome-Terminal
"\e[OF": end-of-line
"\e[OH": beginning-of-line

"\e[[F": end-of-line
"\e[[H": beginning-of-line

"\e[4~": end-of-line
"\e[1~": beginning-of-line

set show-all-if-ambiguous on
set completion-ignore-case on
